{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "include": ["js/**/*"],
  "exclude": ["node_modules"]
}
